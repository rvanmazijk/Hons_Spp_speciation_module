ars (white with a wide black margin).},
author = {Lew, Daniel and P{\'{e}}rez-Hernandez, Roger and Ventura, Jacint},
doi = {10.1644/05-MAMM-A-065R2.1},
file = {:Users/ruanvanmazijk/Documents/Mendeley Desktop/2006/Lew, P{\'{e}}rez-Hernandez, Ventura{\_}2006.pdf:pdf},
issn = {0022-2372},
journal = {Journal of Mammalogy},
keywords = {Didelphimorphia,Neotropics,morphometrics,new species,taxonomy},
number = {2},
pages = {224--237},
title = {{Two New Species of Philander ( Didelphimorphia , Didelphidae ) From Northern South America}},
url = {https://oup.silverchair-cdn.com/oup/backfile/Content{\_}public/Journal/jmammal/87/2/10.1644/05-MAMM-A-065R2.1/2/87-2-224.pdf},
