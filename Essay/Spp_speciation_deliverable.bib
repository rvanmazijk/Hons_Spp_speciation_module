@article{Grenyer2003,
abstract = {The first MRP (matrix representation with parsimony) supertree phylogeny of the Lipotyphla is presented, covering$\backslash$nall the families that were considered to make up the traditional mammalian order Insectivora. The phylogeny$\backslash$ndoes not examine relationships within the shrew subfamily Crocidurinae, but all other taxa are considered at$\backslash$nthe species level, drawing upon 41 years of systematic literature and combining information from 47 published$\backslash$nsources. The MRP technique is also critically discussed. This study will be of use to comparative biology studies$\backslash$nof the Lipotyphla (or of mammals as a whole) and is a rigorous review of past systematic work, as well as clearly$\backslash$ndemonstrating our current level of knowledge. The supertree clearly details a strong imbalance in phylogenetic$\backslash$nunderstanding across the taxon: a great deal is known about the hedgehogs and gymnures (Erinaceidae), the New$\backslash$nWorld moles (Talpidae), Palaearctic species of Sorex (subgenus Sorex) and the relationships between genera of$\backslash$nred-toothed shrews (Soricinae). The supertree, however, clearly shows areas where our knowledge is conflicting or$\backslash$nnon-existent, and these gaps do not always correspond to obscure species: nothing is known about the systematics$\backslash$nof Old World mole genera. Also very little is known about golden moles (Chrysochloridae) and the shrew-tenrec$\backslash$ngenus Microgale, some of the most threatened mammals on Earth.},
author = {Grenyer, R and Purvis, a},
doi = {10.1017/S0952836903003716},
file = {:Users/ruanvanmazijk/Documents/Mendeley Desktop/2003/Grenyer, Purvis{\_}2003.pdf:pdf},
isbn = {0952-8369},
issn = {09528369},
journal = {Journal of Zoology},
keywords = {insectivora,lipotyphla,mrp,phylogeny,supertree},
pages = {245--257},
title = {{A composite species-level phylogeny of the ‘Insectivora'$\backslash$n(Mammalia: Order Lipotyphla Haeckel, 1866)}},
volume = {260},
year = {2003}
}
@misc{Springer2004,
abstract = {Deciphering relationships among the orders of placental mammals remains an important problem in evolutionary biology and has implications for understanding patterns of morphological character evolution, reconstructing the ancestral placental genome, and evaluating the role of plate tectonics and dispersal in the biogeographic history of this group. Until recently, both molecular and morphological studies provided only a limited and questionable resolution of placental relationships. Studies based on larger and more diverse molecular datasets, and using an array of methodological approaches, are now converging on a stable tree topology with four major groups of placental mammals. The emerging tree has revealed numerous instances of convergent evolution and suggests a role for plate tectonics in the early evolutionary history of placental mammals. The reconstruction of mammalian phylogeny illustrates both the pitfalls and the powers of molecular systematics.},
author = {Springer, Mark S. and Stanhope, Michael J. and Madsen, Ole and {De Jong}, Wilfried W.},
booktitle = {Trends in Ecology and Evolution},
doi = {10.1016/j.tree.2004.05.006},
file = {:Users/ruanvanmazijk/Documents/Mendeley Desktop/2004/Springer et al.{\_}2004.pdf:pdf},
